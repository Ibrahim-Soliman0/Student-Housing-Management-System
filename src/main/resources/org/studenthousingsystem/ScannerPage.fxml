<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="450.0" style="-fx-background-color: #40577a;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.studenthousingsystem.ScannerController">
   <children>
      <Button fx:id="scanEntrance" layoutX="118.0" layoutY="128.0" mnemonicParsing="false" onAction="#onScanEntranceClick" prefHeight="63.0" prefWidth="214.0" text="Scan Entrance" textFill="#29d490">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
      </Button>
      <Button fx:id="scanExit" layoutX="146.0" layoutY="258.0" mnemonicParsing="false" onAction="#onScanExitClick" prefHeight="63.0" prefWidth="158.0" text="Scan Exit" textFill="#29d490">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
      </Button>
   </children>
</AnchorPane>
